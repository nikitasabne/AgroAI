# AgroAI API Configuration
# Copy this file to .env and add your API keys

# OpenWeatherMap API (Required for weather data)
# Get your free API key from: https://openweathermap.org/api
OPENWEATHER_API_KEY=your_openweather_api_key_here

# India Government Data API (Optional - for market prices)
# Get API key from: https://data.gov.in/
DATA_GOV_IN_KEY=your_data_gov_in_api_key_here

# Application Configuration
NODE_ENV=development
PORT=4200

# Future API Keys (as you expand)
# AGMARKNET_API_KEY=your_agmarknet_key
# ENOM_API_KEY=your_enom_key
# SMS_API_KEY=your_sms_service_key
# PAYMENT_GATEWAY_KEY=your_payment_key
